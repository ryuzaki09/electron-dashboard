{
  "name": "webbabelereact",
  "version": "1.0.0",
  "description": "template for electron and react",
  "main": "build/index.js",
  "scripts": {
    "build": "rm -rf ./dist && webpack --config webpack.config.js",
    "build-main": "rm -rf ./build && webpack --config webpack.config.electron.js -r @babel/register",
    "start": "npm run build && electron .",
    "test": "jest test/unit/",
    "test:acceptance": "mocha -r @babel/register test/acceptance/ --timeout 15000",
    "lint": "eslint . --ext .js,.ts,.tsx",
    "start-electron": "electron -r ./babel-register.cjs ./index.js",
    "oldstart-electron": "electron -r ./babel-register.js ./index.ts",
    "dev": "BROWSER=none rm -rf ./dist/ && node -r ./babel-register.cjs server/server.js",
    "clean:install": "rm -rf node_modules && npm install",
    "tsc": "tsc",
    "devapp": "node -r ./babel-register.cjs server/server.js"
  },
  "keywords": [
    "electron",
    "react",
    "babel",
    "webpack"
  ],
  "author": "Sinister",
  "license": "ISC",
  "dependencies": {
    "axios": "1.8.3",
    "dotenv": "^16.4.7",
    "electron": "30.5.1",
    "electron-context-menu": "2.5.2",
    "express": "4.16.4",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-router-dom": "7.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "7.9.0",
    "@babel/register": "7.25.9",
    "@ryusenpai/shared-components": "1.13.10",
    "@testing-library/react": "16.2.0",
    "@types/express": "4.17.21",
    "@types/jest": "25.2.1",
    "@types/react": "19.0.8",
    "@types/react-dom": "19.0.3",
    "@types/webpack-dev-middleware": "4.3.0",
    "@types/webpack-hot-middleware": "2.25.9",
    "@typescript-eslint/eslint-plugin": "3.6.1",
    "@typescript-eslint/parser": "3.6.1",
    "babel-eslint": "10.0.1",
    "babel-loader": "^10.0.0",
    "babel-register": "6.26.0",
    "body-parser": "1.20.3",
    "chalk": "^4.1.0",
    "css-loader": "^3.1.0",
    "date-fns": "4.1.0",
    "electron-devtools-installer": "4.0.0",
    "eslint": "5.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-standard": "^4.0.0",
    "html-webpack-plugin": "^4.5.0",
    "jest": "25.3.0",
    "mini-css-extract-plugin": "2.9.2",
    "mocha": "7.2.0",
    "prettier": "1.14.3",
    "sass-loader": "16.0.5",
    "style-loader": "0.23.1",
    "ts-node": "10.9.2",
    "typescript": "3.8.3",
    "webpack": "5.98.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-middleware": "4.3.0",
    "webpack-hot-middleware": "2.24.3",
    "webpack-merge": "5.0.9"
  },
  "jest": {
    "roots": [
      "test"
    ]
  }
}
